        "print(x ** z)  # Exponentiation\n",
        "print(x / z)  # Division\n",
        "print(x // z)  # Floor division\n",
        "print(x % z)  # Modulo\n",
        "\n",
        "# Comparison operators\n",
        "print(x < z)  # Less than\n",
        "print(type(x < z))  # Check type\n",
        "print(x <= z)  # Less than or equal\n",
        "print(x == z)  # Equal\n",
        "print(x > z)  # Greater than\n",
        "print(x >= z)  # Greater than or equal\n",
        "print(x != z)  # Not equal\n",
        "\n",
        "# Logical operators\n",
        "print((108 == 108) and (21 == 21))\n",
        "print((108 == 108) and (21 == 11))  # False condition\n",
        "print((108 == 108) or (21 == 11))\n",
        "\n",
        "# If-Else statements\n",
        "money = 100000\n",
        "print(money > 2000)\n",
        "money = 10000\n",
        "if money > 1000:\n",
        "    print(\"You can enroll in a Data Science course\")\n",
        "\n",
        "# If-Elif-Else\n",
        "test_score = 80\n",
        "if test_score > 80:\n",
        "    print(\"A Grade\")\n",
        "elif 60 < test_score <= 80:\n",
        "    print(\"B Grade\")\n",
        "else:\n",
        "    print(\"Nothing for you\")\n",
