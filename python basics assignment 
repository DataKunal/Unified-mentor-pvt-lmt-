        "for i in range(10):\n",
        "    print(i)\n",
        "\n",
        "# Print 'I love sports' 10 times using a for loop\n",
        "for _ in range(10):\n",
        "    print(\"I love sports\")\n",
        "\n",
        "# While loop\n",
        "number = 0\n",
        "while number < 10:\n",
        "    print(number)\n",
        "    number += 1\n",
        "\n",
        "# Break statement\n",
        "for num in range(10):\n",
        "    if num == 5:\n",
        "        break\n",
        "    print(\"The number is\", num)\n",
        "\n",
        "# Continue statement\n",
        "for num in range(10):\n",
        "    if num == 5:\n",
        "        continue\n",
        "    print(\"The number is\", num)\n",
        "\n",
        "# String manipulation\n",
        "string_ = \"We are creating next generation data science eco-system at CollegeRanker\"\n",
        "print(len(string_))  # Length\n",
        "print(string_[0])  # First character\n",
        "print(string_[-1])  # Last character\n",
        "print(string_[:6])  # Slice from start\n",
        "print(string_[7:-10])  # Slice with negative index\n",
        "\n",
        "# String functions\n",
        "print(string_.count('a'))  # Count occurrences of 'a'\n",
        "print(string_.count(\"data\"))  # Count occurrences of 'data'\n",
        "print(string_.find(\"science\"))  # Find substring\n",
        "print(string_.startswith(\"We\"))  # Check start\n",
        "print(string_.endswith(\"Ranker\"))  # Check end\n",
        "print(string_.upper())  # Convert to uppercase\n",
        "print(string_.capitalize())  # Capitalize first letter\n",
        "print(string_.islower())  # Check if lowercase\n",
        "print(string_.isupper())  # Check if uppercase\n",
        "print(string_.isdigit())  # Check if digit\n",
        "print(string_.isalpha())  # Check if alphabetic\n",
        "print(string_.isalnum())  # Check if alphanumeric\n",
        "\n",
        "# String replacement and split\n",
        "new_str = \"C++ is easy to learn\"\n",
        "print(new_str.replace(\"C++\", \"Python\"))\n",
        "print(new_str.split())\n",
        "\n",
